{
  "name": "@dope-js/markdown-editor",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "scripts": {
    "watch": "pnpm run rollup --watch",
    "build": "run-s clear rollup alias typing clear-temp",
    "rollup": "rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "alias": "tsc-alias -p tsconfig.base.json --outDir ./lib/.typing.temp",
    "typing": "api-extractor run --local --verbose",
    "clear": "rimraf lib",
    "clear-temp": "rimraf lib/.typing.temp"
  },
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "browser": "lib/index.umd.js",
  "types": "src/index.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./lib/index.es.js",
      "require": "./lib/index.cjs.js",
      "browser": "./lib/index.umd.js"
    },
    "./lib/*": "./lib/*",
    "./package.json": "./package.json",
    "./style.css": "./lib/style.css"
  },
  "publishConfig": {
    "types": "lib/index.d.ts",
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@codemirror/language-data": "^6.5.1",
    "@codemirror/state": "^6.5.1",
    "@codemirror/view": "^6.36.2",
    "@douyinfe/semi-ui": "^2.73.0",
    "@floating-ui/react": "^0.27.3",
    "@lexical/list": "^0.23.1",
    "@lexical/markdown": "^0.23.1",
    "@lexical/react": "^0.23.1",
    "@lexical/rich-text": "^0.23.1",
    "@lexical/selection": "^0.23.1",
    "@lexical/utils": "^0.23.1",
    "@mdxeditor/gurx": "^1.2.3",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "cm6-theme-basic-dark": "^0.2.0",
    "cm6-theme-basic-light": "^0.2.0",
    "codemirror": "^6.0.1",
    "katex": "^0.16.20",
    "lexical": "^0.23.1",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-gfm-strikethrough": "^2.0.0",
    "mdast-util-gfm-table": "^2.0.0",
    "mdast-util-gfm-task-list-item": "^2.0.0",
    "mdast-util-math": "^3.0.0",
    "mdast-util-mdx": "^3.0.0",
    "mdast-util-mdx-jsx": "^3.2.0",
    "mdast-util-to-markdown": "^2.1.2",
    "micromark-extension-gfm-strikethrough": "^2.1.0",
    "micromark-extension-gfm-table": "^2.1.0",
    "micromark-extension-gfm-task-list-item": "^2.1.0",
    "micromark-extension-math": "^3.1.0",
    "micromark-extension-mdx-jsx": "^3.0.1",
    "micromark-extension-mdx-md": "^2.0.0",
    "react-katex": "^3.0.1"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@types/mdast": "^4.0.4",
    "@types/react": "18.2.25",
    "@types/react-dom": "18.2.25",
    "@types/react-katex": "^3.0.4",
    "micromark-util-types": "^2.0.1"
  }
}
