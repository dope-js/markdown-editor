{
  "name": "@dope-js/markdown-editor",
  "version": "0.0.12",
  "description": "A wysiwyg markdown editor",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dope-js/markdown-editor"
  },
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "types": "src/index.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./lib/index.es.js",
      "require": "./lib/index.cjs.js"
    },
    "./lib/*": "./lib/*",
    "./package.json": "./package.json",
    "./style.css": "./lib/style.css"
  },
  "publishConfig": {
    "types": "lib/index.d.ts",
    "exports": {
      ".": {
        "types": "./lib/index.d.ts",
        "import": "./lib/index.es.js",
        "require": "./lib/index.cjs.js"
      },
      "./lib/*": "./lib/*",
      "./package.json": "./package.json",
      "./style.css": "./lib/style.css"
    },
    "access": "public",
    "directory": ".publish",
    "registry": "https://registry.npmjs.org/"
  },
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "watch": "pnpm run rollup --watch",
    "build": "run-s clear rollup alias typing clear-temp",
    "rollup": "rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "alias": "tsc-alias -p tsconfig.base.json --outDir ./lib/.typing.temp",
    "typing": "api-extractor run --local --verbose",
    "clear": "rimraf lib",
    "clear-temp": "rimraf lib/.typing.temp",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "clean-publish": "rimraf ./.publish && clean-publish --access public",
    "prepublishOnly": "run-s build clean-publish",
    "postpublish": "rimraf ./.publish",
    "lint": "eslint ."
  },
  "dependencies": {
    "@codemirror/language-data": "^6.5.1",
    "@codemirror/state": "^6.5.1",
    "@codemirror/view": "^6.36.2",
    "@floating-ui/react": "^0.27.3",
    "@lexical/list": "^0.23.1",
    "@lexical/markdown": "^0.23.1",
    "@lexical/react": "^0.23.1",
    "@lexical/rich-text": "^0.23.1",
    "@lexical/selection": "^0.23.1",
    "@lexical/utils": "^0.23.1",
    "@mdx-js/mdx": "^3.0.1",
    "@mdxeditor/gurx": "^1.2.3",
    "clsx": "^2.1.1",
    "cm6-theme-basic-dark": "^0.2.0",
    "cm6-theme-basic-light": "^0.2.0",
    "codemirror": "^6.0.1",
    "katex": "^0.16.20",
    "lexical": "^0.23.1",
    "lodash-es": "^4.17.21",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-gfm-strikethrough": "^2.0.0",
    "mdast-util-gfm-table": "^2.0.0",
    "mdast-util-gfm-task-list-item": "^2.0.0",
    "mdast-util-math": "^3.0.0",
    "mdast-util-mdx": "^3.0.0",
    "mdast-util-mdx-jsx": "^3.2.0",
    "mdast-util-to-markdown": "^2.1.2",
    "micromark-extension-gfm-strikethrough": "^2.1.0",
    "micromark-extension-gfm-table": "^2.1.0",
    "micromark-extension-gfm-task-list-item": "^2.1.0",
    "micromark-extension-math": "^3.1.0",
    "micromark-extension-mdx-jsx": "^3.0.1",
    "micromark-extension-mdx-md": "^2.0.0",
    "prismjs": "^1.29.0",
    "react-katex": "^3.0.1",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@chromatic-com/storybook": "^3.2.4",
    "@commander-js/extra-typings": "^13.0.0",
    "@eslint/js": "^9.18.0",
    "@microsoft/api-extractor": "^7.49.1",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/addon-interactions": "^8.5.0",
    "@storybook/addon-onboarding": "^8.5.0",
    "@storybook/blocks": "^8.5.0",
    "@storybook/react": "^8.5.0",
    "@storybook/react-vite": "^8.5.0",
    "@storybook/test": "^8.5.0",
    "@types/hast": "^3.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.10.7",
    "@types/prismjs": "^1.26.5",
    "@types/react": "18.2.25",
    "@types/react-dom": "18.2.25",
    "@types/react-katex": "^3.0.4",
    "@types/rollup-plugin-peer-deps-external": "^2.2.5",
    "@types/unist": "^3.0.3",
    "autoprefixer": "^10.4.20",
    "clean-publish": "^5.1.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.14.0",
    "micromark-util-types": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.30.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-css-modules": "^0.1.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.1",
    "sass": "^1.83.4",
    "semver": "^7.6.3",
    "shelljs": "^0.8.5",
    "storybook": "^8.5.0",
    "storybook-addon-dark-mode-toggle": "^0.0.6",
    "storybook-dark-mode": "^4.0.2",
    "tsc-alias": "^1.8.10",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}